<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="PPGroup_ZipcodeSuggestion::js/addressAutoFill.js"/>
    </head>

    <body>
        <referenceContainer name="content">
            <block name="inject.config.block" class="Magento\Framework\View\Element\Template"
                   template="PPGroup_ZipcodeSuggestion::inject_config.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">PPGroup\ZipcodeSuggestion\ViewModel\InjectConfig
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>

</page>
