<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Mageplaza_OrderAttributes::css/sales-order-create.css"/>
    </head>
    <body>
        <referenceBlock name="shipping_method">
            <block class="Mageplaza\OrderAttributes\Block\Adminhtml\SalesOrderCreate" name="mporderattributes_sales_order_create_shipping" template="order/create/shipping.phtml"/>
        </referenceBlock>
        <referenceBlock name="billing_method">
            <block class="Mageplaza\OrderAttributes\Block\Adminhtml\SalesOrderCreate" name="mporderattributes_sales_order_create_payment" template="order/create/payment.phtml" after="-"/>
        </referenceBlock>
        <referenceBlock name="data">
            <block class="Mageplaza\OrderAttributes\Block\Adminhtml\SalesOrderCreate" name="mporderattributes_sales_order_create_summary" template="order/create/summary.phtml"/>
        </referenceBlock>
    </body>
</page>
